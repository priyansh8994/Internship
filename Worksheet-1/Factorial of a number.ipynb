   ],
   "source": [
    "num = int(input(\"ENTER A NUMBER: \"))\n",
    "factorial = 1\n",
    "for i in range(1, num + 1):\n",
    "    factorial *= i\n",
    "factorial_rounded = round(factorial, 2)\n",
    "print(\"THE FACTORIAL OF\" , num, \"is\", factorial_rounded)"
   ]
